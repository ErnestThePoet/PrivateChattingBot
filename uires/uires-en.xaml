<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <!-- MainWindow Strings-->
    <sys:String x:Key="AppWindowTitle">
        Ernest's Private Chatting Bot
    </sys:String>

    <sys:String x:Key="PasteOnlyTitle">
        Paste Only
    </sys:String>

    <sys:String x:Key="MessageToSendTitle">
        Message To Send
    </sys:String>

    <sys:String x:Key="SettingsLabel">
        Settings
    </sys:String>

    <sys:String x:Key="ChatTargetListTitle">
        Chat Target List
    </sys:String>

    <sys:String x:Key="FinishedChatTargetListTitle">
        Finished Chat Target List
    </sys:String>

    <sys:String x:Key="UninitializedStateText">
        UNINITIALIZED
    </sys:String>

    <sys:String x:Key="RunningStateText">
        RUNNING
    </sys:String>

    <sys:String x:Key="StopStateText">
        STOPPED
    </sys:String>

    <!-- SettingsWindow Strings-->
    <sys:Double x:Key="SettingsWindowWidth">
        280
    </sys:Double>
    
    <sys:String x:Key="SettingsWindowTitle">
        Settings
    </sys:String>

    <sys:String x:Key="PasteOnlyLabel">
        Paste message only without sending
    </sys:String>

    <sys:String x:Key="ShortIntervalMsLabel">
        Short operation interval(ms):
    </sys:String>

    <sys:String x:Key="LongIntervalMsLabel">
        Long operation interval(ms):
    </sys:String>

    <sys:String x:Key="SearchBarCoordLabel">
        Search bar coordination(X,Y):
    </sys:String>

    <sys:String x:Key="MessageBoxCoordLabel">
        Message box coordination(X,Y):
    </sys:String>

    <sys:String x:Key="ButtonOkText">
        OK
    </sys:String>

    <sys:String x:Key="ButtonCancelText">
        Cancel
    </sys:String>

    <sys:String x:Key="ShortIntervalMsInvalidMessage">
        Short operation interval must be a positive integer
    </sys:String>

    <sys:String x:Key="LongIntervalMsInvalidMessage">
        Long operation interval must be a positive integer
    </sys:String>

    <sys:String x:Key="SearchBarCoordInvalidMessage">
        Search bar coordinations must be positive integers
    </sys:String>

    <sys:String x:Key="MessageBoxCoordInvalidMessage">
        Message box coordinations must be positive integers
    </sys:String>
</ResourceDictionary>